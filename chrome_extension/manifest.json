{
    "manifest_version": 2,
    
    "name": "Vista",
    "description": "Share video content with others",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}

